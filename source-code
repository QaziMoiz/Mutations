
function mutation(arr) {
  var a = arr[0];
  var b = arr[1];
  a = a.toLowerCase();
  b = b.toLowerCase();
  
  var myArr = [];

for(var i = 0; i < b.length; i++){
  if(a.indexOf(b[i]) >= 0){
    myArr.push(b[i]);
  //console.log(true);
    //console.log(myArr);
    if(myArr.length == b.length){
      console.log(true);
    }
}else{
  console.log(false);
      }
}
}

mutation(["hello", "hE"]);
